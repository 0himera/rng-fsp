APP_NAME=RandomTrust RNG Backend
APP_VERSION=0.1.0
API_PREFIX=/api
ENVIRONMENT=development

POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=randomtrust
POSTGRES_USER=randomtrust
POSTGRES_PASSWORD=randomtrust
DATABASE_URL=postgresql+asyncpg://randomtrust:randomtrust@postgres:5432/randomtrust
ALEMBIC_DATABASE_URL=postgresql://randomtrust:randomtrust@postgres:5432/randomtrust

REDIS_URL=redis://redis:6379/0
REDIS_STREAM_ENTROPY=entropy:events
REDIS_STREAM_RESULTS=entropy:results

MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=randomtrust
MINIO_SECRET_KEY=randomtrustsecret
MINIO_BUCKET=entropy-artifacts

RNG_EXPORT_PATH=/data/runs
